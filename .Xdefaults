! Settings
URxvt*termName:             rxvt-unicode
URxvt*loginShell:           false
URxvt*scrollBar:            false
URxvt*saveLines:            10000

!URxvt*keysym.C-Left: \033[1;5D
!URxvt*keysym.C-Right: \033[1;5C
!URxvt*keysym.C-Down: \033[1;5B
!URxvt*keysym.C-Up: \033[1;5A
!URxvt*keysym.C-S-Left: \033[1;6D
!URxvt*keysym.C-S-Right: \033[1;6C
!URxvt*keysym.C-S-Up: \033[1;6B
!URxvt*keysym.C-S-Down: \033[1;6A

!URxvt*shading:              +10
URxvt*geometry:             120x40
URxvt*internalBorder:       0
URxvt*perl-ext-common:      default,matcher,tabbedex
!URxvt.perl-ext-common:      default,tabbed,matcher,selection-pastebin,selection,urxvt-clipboard
URxvt*colorUL:              white
URxvt*urlLauncher:          chromium
URxvt*matcher.button:       1
URxvt*cursorUnderline:      false
!URxvt*font:                 -*-Terminus-medium-r-*-*-14-120-*-*-*-*-*-*
!URxvt*boldFont:             -*-Terminus-medium-r-*-*-14-120-*-*-*-*-*-*
!URxvt*font:                 xft:Terminus:size=12:autohint=true
!URxvt*boldFont:             xft:Terminus:bold:size=12:autohint=true
!URxvt*italicFont:           xft:Terminus:italic:size=12:autohint=true
!URxvt*boldItalicFont:       xft:Terminus:bold:italic:size=12:autohint=true
URxvt*font:                 xft:UbuntuMono-RI:size=13:autohint=true
URxvt*boldFont:             xft:UbuntuMono-RI:bold:size=13:autohint=true
URxvt*italicFont:           xft:UbuntuMono-RI:italic:size=13:autohint=true
URxvt*boldItalicFont:       xft:UbuntuMono-RI:bold:italic:size=13:autohint=true
URxvt*scrollTtyOutput:      false
URxvt*scrollTtyKeypress:    true
URxvt*scrollKey:            true
URxvt*scrollWithBuffer:     false
Xft.dpi:                    100  
! URxvt.perl-ext:mark-urls,default

!URxvt.tabbed.tabbar-fg:     1
!URxvt.tabbed.tabbar-bg:     8
!URxvt.tabbed.tab-fg:        10
!URxvt.tabbed.tab-bg:        8

!URxvt.keysym.C-M-e: xsel -o | pastebinit -b "http://paste.ubuntu.com" | xclip -i\n
URxvt.keysym.C-M-e: perl:selection-pastebin:remote-pastebin
!URxvt.selection-pastebin.cmd: rsync -apP % ruth:/var/www/www.ta-sa.org/files/
!URxvt.selection-pastebin.url: http://www.ta-sa.org/files/%
URxvt.selection-pastebin.cmd: pastebinit -b "http://paste.ubuntu.com" -i % | xclip -i
!URxvt.selection-pastebin.url: %

XTerm*termName: xterm-256color
! Tabbedex features
URxvt.new-button:       no
URxvt.title:       limbo
URxvt.autohide:         no
URxvt.no-tabbedex-keys: yes
! !
! ! ! Tabbedex colors
! !URxvt.tabbar-fg: 244
! !URxvt.tabbar-bg: 16
URxvt.tab-fg:    255
URxvt.tab-bg:    10
! !URxvt.title-fg:  244
! !URxvt.title-bg:  16
! !
! ! ! Tabbedex keybindings (4-1: Super + Alt)
URxvt.keysym.Control-`:  perl:tabbedex:rename_tab
URxvt.keysym.Shift-Up:    perl:tabbedex:new_tab
! !URxvt.keysym.4-1-Left:    perl:tabbedex:prev_tab
! !URxvt.keysym.4-1-Right:   perl:tabbedex:next_tab
! !URxvt.keysym.Shift-Ctrl-Left:  perl:tabbedex:move_tab_left
! !URxvt.keysym.Shift-Ctrl-Right: perl:tabbedex:move_tab_right

